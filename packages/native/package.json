    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-native-material-bottom-navigation": "^1.0.1",
    "react-native-modal": "^6.5.0",
    "react-navigation": "^2.18.0"
